{
	"background": {
		"scripts": [ "options.js", "background.js" ]
	},
	"browser_action": {
		"default_icon": "icon_dict.gif",
		"default_popup": "options.html"
	},
	"content_scripts": [ {
		"js": [ "lookup.js" ],
		"matches": [ "http://*/*", "https://*/*" ]
	} ],
	"description": "可记录、导出查询历史!",
	"icons": {
		"128": "icon-128.png",
		"64": "icon-128.png",
		"48": "icon-128.png"
	},
	"manifest_version": 2,
	"name": "有道词典划词扩展 V3",
	"short_name":"有道词典",
	"options_page": "options.html",
	"permissions": [ "http://*/*", "https://*/*" ],
	"version": "3.0.1",
	"web_accessible_resources": [ "ab20.png", "ydd-sprite.png" ]
}